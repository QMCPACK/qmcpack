&CONTROL
   calculation     = 'scf'
   disk_io         = 'low'
   outdir          = 'pwscf_output'
   prefix          = 'pwscf'
   pseudo_dir      = './'
   restart_mode    = 'from_scratch'
   tprnfor         = .true.
   tstress         = .true.
   verbosity       = 'low'
   wf_collect      = .true.
/

&SYSTEM
   celldm(1)       = 1.0
   degauss         = 0.0001
   ecutrho         = 200
   ecutwfc         = 50
   ibrav           = 0
   input_dft       = 'pbe'
   nat             = 2
   nosym           = .false.
   ntyp            = 1
   occupations     = 'smearing'
   smearing        = 'fermi-dirac'
   tot_charge      = 0
/

&ELECTRONS
   conv_thr        = 1e-08
   diagonalization = 'david'
   electron_maxstep = 1000
   mixing_beta     = 0.7
   mixing_mode     = 'plain'
/


