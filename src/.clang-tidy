Checks:          '-*,readability-identifier-naming,modernize-use-override,performance-noexcept-move-constructor'
WarningsAsErrors: ''
FormatStyle:     file
HeaderFilterRegex: 'src/.*'
CheckOptions: [ { key: readability-identifier-naming.NamespaceCase, value: lower_case },
                { key: readability-identifier-naming.ClassCase, value: CamelCase },
                { key: readability-identifier-naming.VariableCase, value: lower_case },
                { key: readability-identifier-naming.ClassMethodCase, value: camelBack },
                { key: readability-identifier-naming.PrivateMemberPostfix, value: _ }]
      
